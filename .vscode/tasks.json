{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "serve",
      "type": "npm",
      "script": "serve",
      "isBackground": true,
      "problemMatcher": {
        "owner": "custom",
        "pattern": {
          "regexp": "^$"
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^\\s*INFO\\s*Starting development server",
          "endsPattern": "^\\s*INFO\\s*App running at"
        }
      }
    }
  ]
}